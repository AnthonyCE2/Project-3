USE DATABASE wholeThing

CREATE TABLE Users {
  userID: int 
  FirstName*
  LastName*
  displayName -- name to display with user post
  email0*
  showEmails -- boolean
  email1
  telno
  showTelno -- boolean
  address1
  address2
  showStreet -- boolean
  city
  showCity -- boolean
  state
  showState -- boolean
  zip
  showZip -- boolean
  password -- encrypted through passportjs
  myPosts -- link to array of posts (however we handle this)
  createdAt
  updatedAt
}